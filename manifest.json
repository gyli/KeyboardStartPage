{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "0.4.0",
  "minimum_chrome_version": "88",
  "permissions": [ "storage", "background" ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_page": "options.html",
  "icons": {
    "32": "img/icon.ico"
  }
}
