{
  "manifest_version": 2,
  "name": "Keyboard Start Page",
  "description": "Open your favorite websites with visualized keyboard shortcuts.",
  "version": "0.2.3",
  "minimum_chrome_version": "33",
  "permissions": [ "tabs", "storage", "background" ],
  "background": {
    "scripts": ["js/jquery-2.1.3.min.js", "background.js"]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}